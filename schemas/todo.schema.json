{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/todo.schema.json",
  "type": "object",
  "title": "ToDo Schema",
  "required": [
    "userId",
    "id",
    "title",
    "completed"
  ],
  "properties": {
    "userId": {
      "$id": "#/properties/userId",
      "type": "integer",
      "title": "Userid",
      "default": 0,
      "examples": [
        1
      ]
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "ToDo Id",
      "default": 0,
      "examples": [
        1
      ]
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "Title",
      "default": "",
      "examples": [
        "delectus aut autem"
      ],
      "pattern": "^(.*)$"
    },
    "completed": {
      "$id": "#/properties/completed",
      "type": "boolean",
      "title": "Completed status",
      "default": false,
      "examples": [
        false
      ]
    }
  }
}
